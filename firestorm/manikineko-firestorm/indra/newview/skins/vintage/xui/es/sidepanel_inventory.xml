			<panel label="Inventario" name="panel_main_inventory"/>
			<panel label="Marketplace" name="marketplace_inbox">
				<string name="InboxLabelWithArg">Items recibidos ([NUM])</string>
				<string name="InboxLabelNoArg">Items recibidos</string>
				<text value="Items recibidos" name="inbox_label"/>
