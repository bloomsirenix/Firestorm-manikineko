<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<floater name="inspect" title="Inspekcja obiektów">
	<floater.string name="Group">
		(Grupa)
	</floater.string>
	<floater.string name="stats_list">
		Statystyki:

Obiekty: [NUM_OBJECTS], primy: [NUM_PRIMS]

Strony: [NUM_FACES]
Widoczne: [NUM_VISIBLE_FACES]
Wierzchołki: [NUM_VERTICES]
Trójkąty: [NUM_TRIANGLES]
Złożoność: [COMPLEXITY]

Tekstury: [NUM_TEXTURES]
VRAM tekstur:
[TEXTURE_MEMORY] KB
VRAM łącznie:
[VRAM_USAGE] KB
	</floater.string>
	<text_editor name="linksetstats_text" value="(ładowanie...)"/>
	<scroll_list name="object_list" tool_tip="Wybierz obiekt z tej listy, aby podświetlić go w świecie">
		<scroll_list.columns label="Nazwa" name="object_name"/>
		<scroll_list.columns label="Opis" name="description"/>
		<scroll_list.columns label="Właściciel" name="owner_name"/>
		<scroll_list.columns label="Twórca" name="creator_name"/>
		<scroll_list.columns label="Strony" name="facecount" />
		<scroll_list.columns label="Wierzchołki" name="vertexcount" />
		<scroll_list.columns label="Trójkąty" name="trianglecount" />
		<scroll_list.columns label="VRAM tekstur" name="tramcount" />
		<scroll_list.columns label="VRAM łącznie" name="vramcount" />
		<scroll_list.columns label="Utworzony" name="creation_date"/>
	</scroll_list>
	<button label="Profil Właściciela..." name="button owner" tool_tip="Zobacz profil Właściciela podświetlonego obiektu"/>
	<button label="Profil Twórcy..." name="button creator" tool_tip="Zobacz profil Twórcy podświetlonego obiektu"/>
	<menu_button name="options_btn" tool_tip="Opcje" />
</floater>
