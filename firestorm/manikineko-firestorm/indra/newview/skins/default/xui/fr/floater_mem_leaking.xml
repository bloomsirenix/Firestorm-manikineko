<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<floater name="MemLeak" title="Simuler une fuite de mémoire">
	<spinner label="Vitesse de la fuite (octets par trame) :" name="leak_speed"/>
	<spinner label="Max mémoire perdue (MO):" name="max_leak"/>
	<text name="total_leaked_label">Mémoire perdue actuellement : [SIZE] KO</text>
	<button label="Démarrer" name="start_btn"/>
	<button label="Arrêter" name="stop_btn"/>
	<button label="Libérer" name="release_btn"/>
	<button label="Fermer" name="close_btn"/>
</floater>
