<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel name="contact_sets_panel">
	<panel.string name="empty_list">
		Этот набор является пустым.
	</panel.string>
	<panel.string name="no_sets">
		Нет наборов
	</panel.string>
	<panel.string name="all_sets">
		Все наборы
	</panel.string>
	<panel.string name="pseudonyms">
		Алиасы
	</panel.string>
	<panel.string name="non_friends">
		Не в списке друзей
	</panel.string>
	<button name="config_btn" tool_tip="Конфигурация выбранного набора" />
	<button name="add_set_btn" tool_tip="Добавить новый набор контактов" />
	<button name="remove_set_btn" tool_tip="Удалить выбранный набор контактов" />
	<button label="Добавить..." name="add_btn" tool_tip="Добавить аватара в текущей набор контактов" />
