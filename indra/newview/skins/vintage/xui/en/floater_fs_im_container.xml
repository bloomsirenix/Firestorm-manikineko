<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<multi_floater
 can_close="true"
 can_minimize="true"
 can_resize="true"
 height="390"
 hosted_floater_show_titlebar="false"
 layout="topleft"
 name="floater_im_box"
 save_rect="true"
 save_visibility="true"
 save_dock_state="true"
 single_instance="true"
 reuse_instance="true"
 title="Conversations"
 width="396">
    <tab_container
     follows="left|right|top|bottom"
     height="390"
     label_pad_left="10"
     layout="topleft"
     left="1"
     name="im_box_tab_container"
     tab_position="left"
     open_tabs_on_drag_and_drop="true"
     tab_width="115"
     tab_max_width = "115"
     tab_height="20"
     use_custom_icon_ctrl="false"
     tab_icon_ctrl_pad="2"
     halign="left"
     use_ellipses="true"
     top="0"
     width="394">
      <first_tab
       tab_bottom_image_flash="Toolbar_Left_Flash"/>
      <middle_tab
       tab_bottom_image_flash="Toolbar_Middle_Flash"/>
      <last_tab
       tab_bottom_image_flash="Toolbar_Right_Flash"/>
    </tab_container>
</multi_floater>
