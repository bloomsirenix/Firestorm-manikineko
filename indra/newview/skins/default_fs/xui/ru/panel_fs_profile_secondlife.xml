        <layout_panel name="right_buttonstack">
            <button name="add_friend" label="Добавить в друзья" label_selected="Добавить в друзья" tool_tip="Предложить дружбу данному жителю" />
            <button name="block" label="Заблокировать" tool_tip="Заблокировть этого жителя" />
            <button name="unblock" label="Разблокировать" tool_tip="Разблокировать этого жителя" />
