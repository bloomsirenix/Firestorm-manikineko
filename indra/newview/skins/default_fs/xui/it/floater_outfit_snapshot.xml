<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<floater name="outfit_snapshot" title="Foto Abito">
	<floater.string name="unknown">
		senzanome
	</floater.string>
	<string name="inventory_progress_str">
		Salvare in inventario
	</string>
	<string name="inventory_succeeded_str">
		Salvata in inventario.
	</string>
	<string name="inventory_failed_str">
		Impossibile salvare in inventario.
	</string>
	<view name="controls_container">
		<button label="Ricarica" name="new_snapshot_btn"/>
		<panel name="advanced_options_panel">
			<text name="layer_type_label">
				Cattura:
			</text>
			<combo_box label="Elementi immagine" name="layer_types">
				<combo_box.item label="Colori" name="Colors"/>
				<combo_box.item label="Profondità" name="Depth"/>
				<combo_box.item label="Profondità (24 bit)" name="Depth24"/>
			</combo_box>
			<check_box label="Interfaccia" name="ui_check" />
			<check_box label="Credito L$" name="currency_check" />
			<check_box label="HUD" name="hud_check" />
			<check_box label="Schermata (tutto lo schermo)" name="freeze_frame_check" />
			<check_box label="Auto-ricarica" name="auto_snapshot_check" />
			<text name="filter_list_label">
				Filtro:
			</text>
			<combo_box name="filters_combobox" tool_tip="Filtri immagine">
				<combo_box.item label="Nessun filtro" name="NoFilter"/>
			</combo_box>
		</panel>
		<panel name="succeeded_panel">
			<text name="succeeded_lbl">
				Salvata
			</text>
		</panel>
		<panel name="failed_panel">
			<text name="failed_lbl">
				Non salvata
			</text>
		</panel>
		<text name="working_lbl">
			In corso
		</text>
		<text name="refresh_lbl">
			Ricarica per salvare.
		</text>
	</view>
	<text name="image_res_text">
		[WIDTH]px (larghezza) x [HEIGHT]px (altezza)
	</text>
	<text name="file_size_label">
		[SIZE] KB
	</text>
</floater>
