<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<panel name="panel_profile" label="Profil">
	<string name="status_online">Connecté</string>
	<string name="status_offline">Déconnecté</string>
	<string name="no_partner_text">Aucun</string>
	<string name="no_group_text">Aucun</string>
	<text name="Name:">Nom :</text>
	<button name="set_name" tool_tip="Définir le nom d'affichage"/>
	<text name="Key:">UUID :</text>
	<text name="label">Né le :</text>
	<text name="label2">Compte :</text>
	<text name="partner_label">Partenaire :</text>
	<text name="Groups:">Groupes :</text>
	<button name="group_invite" label="+" label_selected="+" tool_tip="Inviter dans un groupe"/>
	<group_list name="group_list" for_agent="false"/>
	<text name="About:">À propos :</text>
	<text name="Give item:">Partager :</text>
	<text name="Give inventory" tool_tip="Faites glisser des objets pour les donner à cette personne.">Glissez un objet pour l'offrir.</text>
	<layout_stack name="buttonstack">
		<layout_panel name="left_buttonstack">
			<button name="show_on_map_btn" label="Trouver sur la carte" label_selected="Trouver sur la carte" tool_tip="Localiser cette personne sur la carte"/>
			<button name="pay" label="Payer" label_selected="Payer" tool_tip="Envoyer de l'argent à cette personne"/>
		</layout_panel>
		<layout_panel name="middle_buttonstack">
			<button name="teleport" label="Téléporter" label_selected="Téléporter" tool_tip="Proposer de téléporter cette personne"/>
			<button name="im" label="Message instantané" label_selected="Message instantané" tool_tip="Ouvrir une session de conversation privée avec cette personne"/>
		</layout_panel>
		<layout_panel name="right_buttonstack">
			<button name="add_friend" label="Devenir amis" label_selected="Devenir amis" tool_tip="Devenir ami avec cette personne"/>
			<button name="block" label="Ignorer" tool_tip="Ignorer ce résident"/>
			<button name="unblock" label="Cesser d'ignorer" tool_tip="Cesser d'ignorer cette personne"/>
		</layout_panel>
	</layout_stack>
	<check_box name="show_in_search_checkbox" label="Afficher dans la recherche"/>
</panel>
