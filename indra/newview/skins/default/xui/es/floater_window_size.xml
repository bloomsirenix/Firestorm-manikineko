<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<floater name="window_size" title="TAMAÑO DE LA VENTANA">
	<string name="resolution_format">
		[RES_X] x [RES_Y]
	</string>
	<text name="windowsize_text">
		Definir el tamaño de la ventana:
	</text>
	<combo_box name="window_size_combo" tool_tip="ancho x alto">
		<combo_box.item label="1000 x 700 (por defecto)" name="item1"/>
		<combo_box.item label="1024 x 768 (4:3 XGA)" name="item2"/>
		<combo_box.item label="1280 x 720 (16:9 HD)" name="item3"/>
		<combo_box.item label="1280 x 800 (16:10 WXGA)" name="item4"/>
		<combo_box.item label="1360 x 768 (16:9 WXGA)" name="item5"/>
		<combo_box.item label="1366 x 768 (16:9 WXGA)" name="item7"/>
		<combo_box.item label="1440 x 900 (16:10 WXGA+)" name="item8"/>
		<combo_box.item label="1536 x 864 (16:9)" name="item9"/>
		<combo_box.item label="1600 x 900 (16:9 HD+)" name="item10"/>
		<combo_box.item label="1680 x 1050 (16:10 WSXGA+)" name="item11"/>
		<combo_box.item label="1920 x 1080 (16:9 FHD)" name="item12"/>
		<combo_box.item label="1920 x 1200 (16:10 WUXGA)" name="item13"/>
		<combo_box.item label="2560 x 1440 (16:9 QHD)" name="item14"/>
	</combo_box>
	<button label="Configurar" name="set_btn"/>
	<button label="Cancelar" name="cancel_btn"/>
</floater>
