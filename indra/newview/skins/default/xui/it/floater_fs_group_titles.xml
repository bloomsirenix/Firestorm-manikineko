<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<floater name="group_titles" title="Titoli dei gruppi">
	<floater.string name="NoGroupTitle" value="&lt;Nessun titolo del gruppo&gt;" />
	<filter_editor label="Filtra titoli dei gruppi" name="filter_input" />
	<scroll_list name="title_list">
		<column name="grouptitle" label="Titolo" />
		<column name="groupname" label="Gruppo" />
	</scroll_list>
	<button tool_tip="Aggiorna la lista dei titoli disponibili" name="btnRefresh" />
	<button name="btnInfo" tool_tip="Mostra il profilo del gruppo del titolo selezionato" />
	<button name="btnActivate" label="Attiva" tool_tip="Attiva il titolo selezionato" />
</floater>
