<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<panel label="設定" name="Input panel">
	<tab_container name="tabs">
		<panel label="網路" name="tab-conn">
			<text name="Maximum bandwidth">
				最大頻寬 ([https://wiki.firestormviewer.org/fs_speedtest 你需要一个建议?]):
			</text>
			<check_box label="自訂埠" name="connection_port_enabled"/>
			<spinner label="埠號:" name="connection_port"/>
			<text name="Web:">
				網頁:
			</text>
			<check_box label="接受 cookies" name="cookies_enabled"/>
			<check_box label="啟用 Javascript" name="browser_javascript_enabled"/>
			<check_box label="啟用媒體瀏覽的突顯式視窗" name="media_popup_enabled"/>
			<button label="代理伺服器設定" label_selected="代理伺服器設定" name="set_proxy"/>
		</panel>
		<panel label="地点" name="tab-dir" />
	</tab_container>
</panel>
