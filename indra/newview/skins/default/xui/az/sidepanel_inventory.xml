<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<panel label="Əşyalar" name="objects panel">
	<panel name="sidepanel_inventory_panel">
		<layout_stack name="inventory_layout_stack">
			<layout_panel name="inbox_layout_panel">
				<panel name="marketplace_inbox">
					<string name="InboxLabelWithArg">
						Alınan əşyalar ([NUM])
					</string>
					<string name="InboxLabelNoArg">
						Alınan əşyalar
					</string>
					<button label="Alınan əşyalar" name="inbox_btn"/>
					<text name="inbox_fresh_new_count">
						Yeni: [NUM]
					</text>
					<panel name="inbox_inventory_placeholder_panel" tool_tip="Onları istifadə etmək üçün elementləri avadanlıq siyahınıza daşıyın">
						<text name="inbox_inventory_placeholder">
							Ticarət mərkəzindən alışlar buraya çatdırılacaqlar.
						</text>
					</panel>
				</panel>
			</layout_panel>
		</layout_stack>
		<panel name="button_panel">
			<layout_stack name="button_panel_ls">
				<layout_panel name="info_btn_lp">
					<button label="Profil" name="info_btn" tool_tip="Obyektin profilini göstər"/>
				</layout_panel>
				<layout_panel name="share_btn_lp">
					<button label="Paylaş" name="share_btn" tool_tip="Avadanlıq siyahısından obyekt ilə paylaş"/>
				</layout_panel>
				<layout_panel name="shop_btn_lp">
					<button label="Almaq" name="shop_btn" tool_tip="Ticarət Mərkəzinin pəncərəsini aç"/>
					<button label="Geyin" name="wear_btn" tool_tip="Seçilmiş geyimi geyin"/>
					<button label="Oynat" name="play_btn"/>
					<button label="Teleport" name="teleport_btn" tool_tip="Seçilmiş əraziyə teleport"/>
				</layout_panel>
			</layout_stack>
		</panel>
	</panel>
</panel>
