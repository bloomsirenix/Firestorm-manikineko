<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<floater name="floater_translation_settings" title="Söhbətin tərcümə seçimləri">
	<string name="bing_api_key_not_verified">
		Bing appID açarı təsdiqlənməyib. Yenidən cəhd edin.
	</string>
	<string name="google_api_key_not_verified">
		Google API açarı təsdiqlənməyib. Yenidən cəhd edin.
	</string>
	<string name="bing_api_key_verified">
		Bing appID açarı təsdiqlənib.
	</string>
	<string name="google_api_key_verified">
		Google API açarı təsdiqlənib.
	</string>
	<check_box label="Söhbət zamanı maşın tərcüməsini aktiv edin" name="translate_chat_checkbox"/>
	<text name="translate_language_label">
		Söhbətin tərcümə dili:
	</text>
	<combo_box name="translate_language_combo">
		<combo_box.item label="sistem" name="System Default Language"/>
		<combo_box.item label="ingilis" name="English"/>
		<combo_box.item label="danimarka" name="Danish"/>
		<combo_box.item label="alman" name="German"/>
		<combo_box.item label="ispan" name="Spanish"/>
		<combo_box.item label="fransız" name="French"/>
		<combo_box.item label="italyan" name="Italian"/>
		<combo_box.item label="macar" name="Hungarian"/>
		<combo_box.item label="holland" name="Dutch"/>
		<combo_box.item label="polonya" name="Polish"/>
		<combo_box.item label="portuqal" name="Portugese"/>
		<combo_box.item label="rus" name="Russian"/>
		<combo_box.item label="türk" name="Turkish"/>
		<combo_box.item label="ukrayna" name="Ukrainian"/>
		<combo_box.item label="çin" name="Chinese"/>
		<combo_box.item label="yapon" name="Japanese"/>
		<combo_box.item label="koreya" name="Korean"/>
	</combo_box>
	<text name="tip">
		Tərcümə xidmətini seçin:
	</text>
	<radio_group name="translation_service_rg">
		<radio_item initial_value="bing" label="Bing Translator" name="bing"/>
		<radio_item initial_value="google" label="Google Translate" name="google"/>
	</radio_group>
	<text name="bing_api_key_label">
		Bing [http://www.bing.com/developers/createapp.aspx AppID]:
	</text>
	<button label="Təsdiq et" name="verify_bing_api_key_btn"/>
	<text name="google_api_key_label">
		Google [http://code.google.com/apis/language/translate/v2/getting_started.html#auth API açarı]:
	</text>
	<button label="Təsdiq et" name="verify_google_api_key_btn"/>
	<text name="google_links_text">
		[http://code.google.com/apis/language/translate/v2/pricing.html Qiymətlər] | [https://code.google.com/apis/console Statistika]
	</text>
	<button label="OK" name="ok_btn"/>
	<button label="Ləğv et" name="cancel_btn"/>
</floater>
