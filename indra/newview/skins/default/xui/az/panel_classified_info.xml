			<layout_panel name="layout_panel1">
				<button label="Teleport" name="teleport_btn"/>
			</layout_panel>
			<layout_panel name="show_on_map_btn_lp">
				<button label="Xəritə" name="show_on_map_btn"/>
			</layout_panel>
			<layout_panel name="edit_btn_lp">
