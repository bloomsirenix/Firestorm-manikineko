<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<panel name="panel_classified_info">
	<panel.string name="type_mature">Modéré</panel.string>
	<panel.string name="type_pg">Contenu Général</panel.string>
	<panel.string name="l$_price">L$[PRICE]</panel.string>
	<panel.string name="click_through_text_fmt">[TELEPORT] téléporter, [MAP] carte, [PROFILE] profil</panel.string>
	<panel.string name="date_fmt">[day,datetime,slt]/[mthnum,datetime,slt]/[year,datetime,slt]</panel.string>
	<panel.string name="auto_renew_on">Activé</panel.string>
	<panel.string name="auto_renew_off">Désactivé</panel.string>
	<text name="title">Infos sur l'annonce</text>
	<scroll_container name="profile_scroll">
		<panel name="scroll_content_panel">
			<text_editor name="classified_name">(Nom)</text_editor>
			<text name="classified_location_label">Emplacement :</text>
			<text_editor name="classified_location">(Chargement)</text_editor>
			<text name="content_type_label">Type de contenu :</text>
			<text_editor name="content_type">(Type de contenu)</text_editor>
			<text name="category_label">Catégorie :</text>
			<text_editor name="category">(Catégorie)</text_editor>
			<text name="creation_date_label">Date de création :</text>
			<text name="price_for_listing_label">Coût de l'annonce :</text>
			<text_editor name="creation_date" tool_tip="Date de création">(Date)</text_editor>
			<text_editor name="price_for_listing" tool_tip="Coût de l'annonce.">(Coût)</text_editor>
			<layout_stack name="descr_stack">
				<layout_panel name="clickthrough_layout_panel">
					<text name="click_through_label">Clics :</text>
					<text_editor name="click_through_text" tool_tip="Parcourir les données en cliquant">(Clics)</text_editor>
				</layout_panel>
				<layout_panel name="price_layout_panel">
					<text name="auto_renew_label">Renouvellement :</text>
					<text name="auto_renew">Activé</text>
				</layout_panel>
				<layout_panel name="descr_layout_panel">
					<text name="classified_desc_label">Description :</text>
					<text name="classified_desc">(Description)</text>
				</layout_panel>
			</layout_stack>
		</panel>
	</scroll_container>
	<panel name="buttons">
		<layout_stack name="layout_stack1">
			<layout_panel name="layout_panel1">
				<button label="Téléporter" name="teleport_btn"/>
			</layout_panel>
			<layout_panel name="show_on_map_btn_lp">
				<button label="Carte" name="show_on_map_btn"/>
			</layout_panel>
			<layout_panel name="edit_btn_lp">
				<button label="Modifier" name="edit_btn"/>
			</layout_panel>
		</layout_stack>
	</panel>
</panel>
