<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<panel label="Configurações" name="Input panel">
	<tab_container name="tabs">
		<panel label="Rede" name="tab-conn">
			<text name="Maximum bandwidth">
				Largura de banda máxima ([https://wiki.firestormviewer.org/fs_speedtest você precisa de uma sugestão?]):
			</text>
			<check_box label="Personalizar porta" name="connection_port_enabled"/>
			<spinner label="Número da porta:" name="connection_port" width="160" label_width="85"/>
			<text name="Web:">
				Web:
			</text>
			<check_box label="Aceitar cookies" name="cookies_enabled"/>
			<check_box label="Habilitar Javascript" name="browser_javascript_enabled"/>
			<check_box label="Ativar pop-ups no navegador de mídia" name="media_popup_enabled"/>
			<button label="Configurações de proxy" label_selected="Configurações de proxy" name="set_proxy"/>
		</panel>
		<panel label="Localizações" name="tab-dir" />
	</tab_container>
</panel>
